<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.kohbus">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="18"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <permission android:name="com.kohbus.permission.C2D_MESSAGE" android:protectionLevel="signature"/>

    <uses-permission android:name="com.kohbus.permission.C2D_MESSAGE"/>
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.USE_CREDENTIALS"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/title_activity_login" android:name="com.kohbus.LoginActivity" android:windowSoftInputMode="adjustResize|stateVisible">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_register" android:name="com.kohbus.RegisterActivity" android:windowSoftInputMode="adjustResize|stateVisible">
        </activity>

        <service android:name="com.kohbus.GCMIntentService"/>

        <receiver android:name="com.google.android.gcm.GCMBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>

                <category android:name="com.kohbus"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>

                <category android:name="com.kohbus"/>
            </intent-filter>
        </receiver>

        <activity android:label="@string/title_activity_test" android:name="com.kohbus.TestActivity">
        </activity>
        <activity android:label="@string/title_activity_profile" android:name="com.kohbus.ProfileActivity" android:parentActivityName="com.kohbus.HomeActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.kohbus.HomeActivity"/>
        </activity>
        <activity android:label="@string/title_activity_home" android:name="com.kohbus.HomeActivity">
        </activity>
        <activity android:label="@string/title_activity_time_booking" android:name="com.kohbus.TimeBookingActivity">
        </activity>
        
        <activity android:name="com.kohbus.MakeABookingActivity">
        </activity>
        
        <activity android:name="com.kohbus.ViewBookingHistory">
        </activity>
        
         <activity android:name="com.kohbus.ViewBookingHistoryMoreInfo">
        </activity>
        
        <service android:name=".GCMIntentService"/>
        
        

        <activity android:launchMode="singleTop" android:name=".RegisterActivity"/>
</application>

</manifest>